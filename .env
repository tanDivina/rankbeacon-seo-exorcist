# Google Gemini API Key (Latest SDK)
GOOGLE_API_KEY=your_gemini_api_key_here

# AI Model Configuration
AI_PROVIDER=gemini
GEMINI_MODEL=gemini-2.5-flash

# Backend URL for MCP server
RANKBEACON_API_URL=http://localhost:8000

# Optional: Database and caching (can skip initially)
DATABASE_URL=postgresql://localhost/rankbeacon
REDIS_URL=redis://localhost:6379

# Development settings
DEBUG=true
LOG_LEVEL=INFO
